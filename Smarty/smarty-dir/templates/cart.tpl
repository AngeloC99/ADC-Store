    <section class="cart-section section-b-space">
        <div class="container">
            {if $prodotti != NULL}
            <div class="row">
                <div class="col-sm-12">
                    <table class="table cart-table table-responsive-xs">
                        <thead>
                            <tr class="table-head">
                                <th scope="col">Immagine</th>
                                <th scope="col">prodotto</th>
                                <th scope="col">prezzo</th>
                                <th scope="col">quantità</th>
                                <th scope="col">modifica</th>
                                <th scope="col">totale</th>
                            </tr>
                        </thead>
                        <tbody>
                            {section name=prod loop=$prodotti}
                                <tr>
                                    <form method="post" action="{$path}GestioneCarrello/modificaQuantita">
                                        <td>
                                            <img src="data:{$prodotti[prod].mime};base64,{$prodotti[prod].image}" width="100" height="100"/>
                                        </td>
                                        <td><a href="{$path}GestioneProdotti/recuperaDettagli/{$prodotti[prod].id}">{$prodotti[prod].nome}</a>
                                        </td>
                                        <td>
                                            <h2>€{$prodotti[prod].prezzo}</h2>
                                        </td>
                                        <td>
                                            <div class="qty-box">
                                                <div class="input-group">
                                                    <input type="hidden" name="idProdotto" value="{$prodotti[prod].id}">
                                                    <input type="number" name="quantita" class="form-control input-number"
                                                           value="{$prodotti[prod].quantita}" min="0" max="{$prodotti[prod].quantitaTot}">
                                                </div>
                                            </div>
                                        </td>
                                        <td>
                                            <input type="submit" value="Aggiorna quantità" class="btn btn-block"/>
                                        <td>
                                            <h2 class="td-color">€ {$prodotti[prod].totProd}</h2>
                                        </td>
                                    </form>
                                </tr>
                            {/section}
                        </tbody>
                    </table>
                    <table class="table cart-table table-responsive-md">
                        <tfoot>
                            <tr>
                                <td>Prezzo totale :</td>
                                <td>
                                    <h2>€ {$prezzoTot}</h2>
                                </td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
            <div class="row cart-buttons">
                <div class="col-6"><a href="{$path}GestioneProdotti/recuperaProdotti" class="btn btn-solid">Continua gli acquisti</a></div>
                <div class="col-6"><a href="{$path}GestioneCarrello/procediOrdine" class="btn btn-solid">Procedi all'ordine</a></div>
            </div>

